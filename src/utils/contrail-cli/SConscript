Import('BuildEnv')
env = BuildEnv.Clone()

subdirs = [
            'contrail_cli',
            'contrail_analytics_cli',
            'contrail_config_cli',
            'contrail_control_cli',
            'contrail_vrouter_cli',
          ]

for dir in subdirs:
    env.SConscript(dir + '/SConscript',
                         exports='BuildEnv')
