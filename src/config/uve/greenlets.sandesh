struct Sandesh_GreenletObject {
    1: string   greenlet_name;
    2: i32      count;
    3: optional string greenlet_launch_cmd;
    4: string   greenlet_traces;
}

request sandesh Sandesh_GreenletObjectReq {
    1: string greenlet_name;
}

response sandesh Sandesh_GreenletObjectListResp {
    1: list<Sandesh_GreenletObject> greenlets;
}

