// This is the structure sent by an application to the Collector to represent
// all or part of the Config state of the UVE on this app instance.
//
struct UvePhysicalRouterConfig {
    1: string                           name (key="ObjectPRouter")
    2: optional bool                    created
    3: optional bool                    deleted
    4: optional string                  ip_address
    5: optional string                  connected_bgp_router
    6: optional string                  product_info
    7: optional bool                    auto_conf_enabled 
    8: optional bool                    netconf_enabled_status 
    9: optional string                  last_commit_time
    10: optional string                 last_commit_duration
    11: optional string                 commit_status_message
    12: optional i32                    total_commits_sent_since_up
    // Add additional items here as needed
}

uve sandesh UvePhysicalRouterConfigTrace {
    1: UvePhysicalRouterConfig              data
}
