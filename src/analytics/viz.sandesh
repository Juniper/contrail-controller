// Analytics table schema defination

// Secondary/SASI Index
enum ColIndexType {
    NONE = 0,
    INDEX,          // secondary index
    CUSTOM_INDEX    // sasi index
}

// SASI Index modes
enum ColIndexMode {
    NONE = 0,
    PREFIX,
    CONTAINS
}

// Description of the column
struct schema_column
{
    1: string    name;
    2: gendb.DbDataType datatype;
    3: bool      key;           // partition-key
    4: bool      clustering;    // clustering-column
    5: ColIndexType index_type;
    6: ColIndexMode index_mode;
}

// Description of the column
struct query_column
{
    1: string name;
    2: string datatype;
    3: bool index;
    4: bool select;
    5: string requiredness;
    6: list<string> suffixes;
    7: list<string> choices;
}

struct query_schema_type
{
    1: string type;
    2: list<query_column> columns;
}

struct query_table
{
    1: string name;
    2: query_schema_type schema;
    3: list<string> columnvalues;
    4: string display_name;
}

struct stat_query_column
{
    1: string name;
    2: string datatype;
    3: bool index;
    4: list<string> suffixes;
    5: string uve_type;
}

struct table_schema
{
    1: string   table_name;
    2: bool     is_static;
    3: bool     is_index_table;
    4: list<schema_column> columns;
    5: map<string, string> column_to_query_column;
    6: map<string, string> query_column_to_column;
    7: map<string, string> index_column_to_column;
}

struct stat_table
{
    6: string display_name;
    1: string stat_type;
    2: string stat_attr;
    3: string obj_table;
    5: list<stat_query_column> attributes;
}

