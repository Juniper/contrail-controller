/**
 * @description: XMPP IQ message reception event
 * @severity: DEBUG
 * @cause: Normal diagnostic message used during software trouble-shooting
 * @action: None
 */
systemlog sandesh XmppIqMessageProcess {
    1: "Received IQ message for node:";
    2: string node;
    3: "Action: ";
    4: string action;
    5: "From: ";
    6: string from;
    7: "To: ";
    8: string to;
    9: "ID: ";
    10: string id;
    11: "SubType: ";
    12: string sub_type;
}

trace sandesh XmppIqMessageProcessTrace {
    1: "Received IQ message for node:";
    2: string node;
    3: "Action: ";
    4: string action;
    5: "From: ";
    6: string from;
    7: "To: ";
    8: string to;
    9: "ID: ";
    10: string id;
    11: "SubType: ";
    12: string sub_type;
}

