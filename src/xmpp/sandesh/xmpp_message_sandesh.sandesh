/**
 * XMPP Messages
 */

/**
 * @description: XMPP Connection creation event
 * @severity: DEBUG
 * @cause: Normal diagnostic message used during software trouble-shooting
 * @action: None
 */
systemlog sandesh XmppConnectionCreate {
    1: "Created Xmpp ";
    2: string type;
    3: " connection from ";
    4: string from;
    5: " To : ";
    6: string to;
}

trace sandesh XmppConnectionCreateTrace {
    1: "Created Xmpp ";
    2: string type;
    3: " connection from ";
    4: string from;
    5: " To : ";
    6: string to;
}

