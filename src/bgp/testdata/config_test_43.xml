            <address>10.1.1.100</address>
            <autonomous-system>64512</autonomous-system>
            <identifier>10.1.1.100</identifier>
            <session to='remote1:0'>
                <remove-private-as>
                    <enabled>true</enabled>
                    <all>true</all>
                    <replace>true</replace>
                </remove-private-as>
            </session>
            <session to='remote2:0'>
                <remove-private-as>
                    <enabled>true</enabled>
                    <all>true</all>
                    <replace>false</replace>
