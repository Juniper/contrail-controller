<?xml version="1.0" encoding="utf-8"?>
<config>
    <routing-policy name='basic'>
        <term>
            <from>
                <prefix>
                    <prefix>2001:db8:85a3::8a2e:370:7335/128</prefix>
                    <type>exact</type>
                </prefix>
                <community>11:13</community>
            </from>
            <then>
                <update>
                    <community>
                        <add><community>11:22</community></add>
                    </community>
                    <local-pref>300</local-pref>
                </update>
            </then>
        </term>
    </routing-policy>
    <routing-instance name="test">
        <routing-policy to="basic">
            <sequence>1.0</sequence>
        </routing-policy>
        <vrf-target>target:1:103</vrf-target>
    </routing-instance>
</config>
