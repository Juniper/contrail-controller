/**
 * Structure definition for holding port information as bitmaps
 */
struct PortBucketBitmap {
    /** Bitmap of UDP source port numbers */
    1: list<u32>   udp_sport_bitmap;
    /** Bitmap of UDP destination port numbers */
    2: list<u32>   udp_dport_bitmap;
    /** Bitmap of TCP source port numbers */
    3: list<u32>   tcp_sport_bitmap;
    /** Bitmap of TCP destination port numbers */
    4: list<u32>   tcp_dport_bitmap;
}
