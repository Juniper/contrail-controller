    5: i32 src_ip_prefix_len;

    /** Destination vn for mirroring */
    6: string dst_vn;
    /** Destination ip prefix for mirroring */
    7: string dst_ip_prefix;
    /** Destination ip prefix length for mirroring */
    8: i32 dst_ip_prefix_len;

