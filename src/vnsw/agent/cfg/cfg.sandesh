traceobject sandesh CfgIntfTrace {
    1: string intf;
    2: string vm_name;
    3: string vm;
    4: string vn;
    5: string ip;
    6: string op;
    7: i32 version;
    8: i32 vlan_id;
    9: string vm_project;
    10: string port_type;
    11: string ip6;
}

// Similar to thrift request for adding and delete ports
request sandesh AddPortReq {
    1: string port_uuid;
    2: string instance_uuid;
    3: string tap_name;
    4: string ip_address;
    5: string vn_uuid;
    6: string mac_address;
    7: optional string vm_name;
    8: optional i16 vlan_id;
    9: optional string vm_project_uuid;
   10: i16 port_type;
   11: optional string ip6_address;
}

response sandesh PortResp {
    1: string resp;
}

request sandesh DeletePortReq {
    1: string port_uuid;
}

traceobject sandesh CfgOpenstackAddPort {
    1: string op;
    2: string port_uuid;
    3: string instance_uuid;
    4: string vn_uuid;
    5: string ip_address;
    6: string tap_name;
    7: string mac_address;
    8: string display_name;
    9: string hostname;
    10: string host;
    11: i32 version;
    12: i16 vlan_id;
    13: string vm_project_uuid;
    14: string port_type;
    15: string ip6_address;
}

traceobject sandesh CfgOpenstackDeletePort {
    1: string op;
    2: string port_uuid;
    3: i32 version;
}

traceobject sandesh CfgOpenstackConnect {
    1: string op;
    2: i32 version;
}

trace sandesh CfgIntfWalkDone {
    1: i32 version;
}

trace sandesh CfgIntfWalkStart {
    1: i32 timer_sec;
    2: i32 version;
}

trace sandesh CfgIntfInfo {
    1: string info;
}
