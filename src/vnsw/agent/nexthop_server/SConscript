# -*- mode: python; -*-

import sys
Import('AgentEnv')
env = AgentEnv.Clone()
env.CppEnableExceptions()

vnswnhserver_sources = ['nexthop_manager.cc']

if sys.platform != 'win32':
    vnswnhserver_sources.append(['nexthop_client.cc', 'nexthop_server.cc'])

vnswnhserver = env.Library('vnswnhserver', vnswnhserver_sources)

env.SConscript('test/SConscript', exports='AgentEnv', duplicate = 0)
