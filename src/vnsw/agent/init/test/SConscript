# -*- mode: python; -*-

import re
Import('AgentEnv')
env = AgentEnv.Clone()

AgentEnv.MakeTestEnv(env)
 
agent_init_suite = []
test_agent_init = AgentEnv.MakeTestCmdSrc(env, 'test_agent_init', ['test_agent_init.cc'], agent_init_suite)
test_vmware = AgentEnv.MakeTestCmdSrc(env, 'test_vmware', ['test_vmware.cc'], agent_init_suite)
 
test = env.TestSuite('agent-test', agent_init_suite)
env.Alias('agent:init', test)
Return('agent_init_suite')

