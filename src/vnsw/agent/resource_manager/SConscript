# -*- mode: python; -*-

import sys
Import('AgentEnv')
env = AgentEnv.Clone()

SandeshGenFiles = env.SandeshGenCpp('resource_manager.sandesh')
SandeshGenFiles += env.SandeshGenCpp('mpls_resource_data.sandesh')
SandeshGenSrcs = env.ExtractCpp(SandeshGenFiles)
sandesh_objs = AgentEnv.BuildExceptionCppObj(env, SandeshGenSrcs)

libresourcemgr = env.Library('resourcemanager', sandesh_objs + 
                                                ['mpls_resource_manager.cc', 
                                                 'resource_manager_factory.cc'])

env.SConscript('test/SConscript', exports='AgentEnv', duplicate = 0)
