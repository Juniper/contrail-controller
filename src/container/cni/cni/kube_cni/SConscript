import os

Import('CfgmEnv')
env = CfgmEnv.Clone()

gobin = env.GoCniBuild('kube_cni')

env.Alias('contrail-k8s-cni', gobin)
env.Alias('install', env.InstallAs(env['INSTALL_BIN'] + '/contrail-k8s-cni', gobin))
env.Default(gobin)
